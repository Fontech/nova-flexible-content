{
    "name": "fontech/nova-flexible-content",
    "description": "Flexible Content & Repeater Fields for Laravel Nova.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "flexible",
        "repeat",
        "group",
        "layout"
    ],
    "authors": [
        {
            "name": "Toon Van den Bos",
            "email": "toon@whitecube.be"
        },
        {
            "name": "Fontech",
            "email": "service@fontech.com.tw"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Fontech\\NovaFlexibleContent\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fontech\\NovaFlexibleContent\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
